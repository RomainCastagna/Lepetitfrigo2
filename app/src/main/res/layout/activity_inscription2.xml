<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inscription2">

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="352dp"
        android:layout_height="32dp"
        android:max="100"
        android:progress="50"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="76dp" />

    <TextView
        android:id="@+id/inscr_progressLogin2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/login"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/inscr_progressNickname2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/infoPerso"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="33dp" />

    <TextView
        android:id="@+id/inscr_progressTerms2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/conditions"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="33dp" />

    <Button
        android:id="@+id/inscr_btn_next2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:onClick="launchSubPart3"
        android:text="Etape Suivante"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="548dp" />

    <EditText
        android:id="@+id/inscr_nickname_input"
        android:layout_width="346dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Nickname"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="168dp" />

    <EditText
        android:id="@+id/inscr_nickname_input2"
        android:layout_width="346dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Nickname"
        android:inputType="textPersonName"
        android:text=""
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="224dp" />

    <DatePicker
        android:id="@+id/datePicker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:calendarViewShown="false"
        android:datePickerMode="spinner"
        android:spinnersShown="true"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="320dp">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vos informations personnelles"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="129dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date de naissance"
        android:textColor="@android:color/black"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="282dp" />

</android.support.constraint.ConstraintLayout>